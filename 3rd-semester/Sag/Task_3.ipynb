   "source": [
    "show(\"Task 3\")\n",
    "A, B = var(\"A B\")\n",
    "\n",
    "A = matrix(QQ, [ [3,8,9] , [-1,0,4] , [1,3,1] ])\n",
    "B = matrix(QQ, [ [-2,4,-6], [2,0,-8], [4,4,2] ])\n",
    "B = (B * B) * 1/2\n",
    "B = B - matrix([[2,1,1], [-1,-1,4], [1,0,3]])\n",
    "B = B / 3\n",
    "\n",
    "show(\"X\",A,\" = \", B)"
   ]
